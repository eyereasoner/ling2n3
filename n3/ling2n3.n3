@prefix lingua: <http://www.w3.org/2000/10/swap/lingua#>.
@prefix aux: <http://www.example.org/>.
@prefix log: <http://www.w3.org/2000/10/swap/log#> .
@prefix : <http://example.org/ns#> .

{
   ?x lingua:backward ?y .

   ?y aux:graph ?body .
   ?x aux:graph ?head .
}
=>
{
   ?head <= ?body .
} .

{
   ?x lingua:forward ?y .

   ?x aux:graph ?premise .
   ?y aux:graph ?conclusion .
}
=>
{
   ?premise => ?conclusion .
} .

{
   ?x lingua:answer ?y .

   ?x aux:graph ?premise .
   ?y aux:graph ?conclusion .
}
=>
{
   ?premise =^ ?conclusion .
} .


